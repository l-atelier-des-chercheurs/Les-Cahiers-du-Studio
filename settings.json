{
  "contentDirname" : "user",
  "metaFileext" : ".txt",
  "folderMetafilename" : "meta",

  "metaDateFormat" : "YYYYMMDD_HHmmss",
  "textEncoding" : "UTF-8",
  "deletedPrefix" : "x_",
  "thumbFolderName" : "_thumbs",
  "deletedFolderName" : "_bin",

  "mediaThumbQuality" : 60,

  "_comment" : "// see http://regexr.com/3d4t8",
  "regexpMatchFolderNames" : "^([^.]+)$",
  "regexpGetFileExtension" : "\\.[^.]*$",
  "regexpRemoveFileExtension" : "(.+?)(\\.[^.]*$|$)",

  "structure" : {
    "media": {
      "created": {
        "type": "Date",
        "default": ""
      },
      "modified": {
        "type": "Date",
        "default": ""
      },
      "type": {
        "type": "String",
        "default": ""
      },
      "color": {
        "type": "String",
        "default": ""
      },
      "authors": {
        "type": "String",
        "default": ""
      },
      "keywords": {
        "type": "String",
        "default": ""
      },
      "public": {
        "type": "Boolean",
        "default": false
      },
      "collapsed": {
        "type": "Boolean",
        "default": false
      },
      "duration" : {
        "type": "Number",
        "default": 0
      },
      "y": {
        "type": "Number",
        "default": 0.2
      },
      "content": {
        "type": "String",
        "default": ""
      }
    }
  }
}