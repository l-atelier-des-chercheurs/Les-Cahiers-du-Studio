{
  "contentDirname" : "user",
  "metaFileext" : ".txt",
  "folderMetafilename" : "meta",

  "metaDateFormat" : "YYYYMMDD_HHmmss",
  "textEncoding" : "UTF-8",
  "deletedPrefix" : "x_",
  "thumbFolderName" : "_thumbs",
  "deletedFolderName" : "_bin",

  "mediaThumbQuality" : 60,

  "_comment" : "// see http://regexr.com/3d4t8",
  "regexpMatchFolderNames" : "^([^.]+)$",
  "regexpGetFileExtension" : "\\.[^.]*$",
  "regexpRemoveFileExtension" : "(.+?)(\\.[^.]*$|$)",

  "structure" : {
    "folder": {


    },
    "media": {
      "date_timeline": {
        "type": "Date"
      },
      "date_created": {
        "type": "Date"
      },
      "date_upload": {
        "type": "Date"
      },
      "date_modified" : {
        "type": "Date"
      },
      "public": {
        "type": "Boolean",
        "default": false
      },
      "color": {
        "type": "String",
        "options": ["white", "red", "blue", "green", "purple", "orange", "yellow"],
        "default": "white"
      },
      "type": {
        "type": "String",
        "options": ["image", "video", "audio", "text", "marker", "other"],
        "default": "other"
      },
      "collapsed": {
        "type": "Boolean",
        "default": false
      },
      "ratio" : {
        "type": "Number",
        "default": 1
      },
      "duration" : {
        "type": "Number"
      },
      "authors": {
        "type": "String"
      },
      "keywords": {
        "type": "String"
      },
      "y": {
        "type": "Number",
        "default": 0.2
      },
      "caption": {
        "type": "String"
      }
    }
  }
}